#include<iostream>

using namespace std;

int main(){
    int first=0,second=1,n,sum;
    cout<<"enter the term of fibbonacci series that you want";
    cin>>n;
    if (n==0)
    cout<<first;
    else if (n==2)
    cout<<second;
    else{
    for(int i=2;i<=n;i++){
       sum=first+second;
       first=second;
       second=sum;
       cout<<sum<<" ";
    }}
    cout<<endl;
    cout<<sum;

    return 0;
}
