#include<iostream>

using namespace std;
int fib(int n){
    if( n==0|| n==1)
    return 1;
    return fib(n-2)+fib(n-1);
     
}

int main(){
    int n;
    cout<<"enter the position"<<endl;
    cin>>n;
    cout<<"the value of fibonacci sequence at position"<<n<<"is" <<fib(n)<<endl;
    
    return 0;
}
